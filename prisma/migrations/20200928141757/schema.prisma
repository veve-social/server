datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

model User {
  id        Int      @default(autoincrement()) @id
  email     String   @unique
  password  String  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
